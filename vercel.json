{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "pages/api/submit.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/thankyou.html", "dest": "/public/thankyou.html" },
    { "src": "/api/submit", "dest": "pages/api/submit.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
